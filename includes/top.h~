struct Control
{
        int nSweep; //a
        int nStep;//b
        int nSample;//c
        double dSweep;//d
        double minF;//e
        double maxF;//f

        char *C_sweep;
        char *C_step;
        char *C_sample;
        char *C_delay;
        char *C_min;
        char *C_max;

        double gain;
        char *C_gain;
};


